<h1>Photos</h1>
<%@photos.each do |photo|%>
    <%if photo.img.attached?%>
        <%= image_tag photo.img %>
        <%= link_to "Destroy",photo_path(photo),data: {turbo_method: :delete,turbo_confirm: "Are you sure" }%>
    <%end%>
<%end%>
<%=link_to "New",new_photo_path%>
<%=link_to "Trash",trash_index_path%>